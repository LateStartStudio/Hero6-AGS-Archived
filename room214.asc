/*  Events  */
function preFade() {
  aMusic26.Play();
  if(isDaytime()) {
  oHorse.Visible = true;
  oHorse.SetView(HORSE_GRAZE);
  oHorse.Animate(0, 6, eRepeat, eNoBlock, eForwards);
  } else {
  oHorse.Visible = false;
  }
}


function exitWest()
{
player.ChangeRoomAutoPosition(213);
}

function onExit()
{
StopMusic();
}

