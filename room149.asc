/*  Events  */
function exitWest()
{
  player.ChangeRoomAutoPosition(140);
}

function exitEast()
{
  player.ChangeRoomAutoPosition(179);
}

function exitSouth()
{
  player.ChangeRoomAutoPosition(165);
}

function exitNorth()
{
  player.ChangeRoom(148, 129, 237);
}

